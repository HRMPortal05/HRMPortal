{
  "builds": [{
      "src": "EMS/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": { "maxLambdaSize": "15mb",
              "runtime": "python3.11"
      }
  },
  {
    "src":"build.sh"
  }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "EMS/wsgi.py"
      }
  ]
}