{
    "version": 2,
    "builds": [
      {
        "src": "EMS/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.11"
        }
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "EMS/wsgi.py"
      }
    ],
    "buildCommand": "pip install -r requirements.txt && ./vercel.sh"
  }
  